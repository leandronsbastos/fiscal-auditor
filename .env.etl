# Configuração do Serviço ETL

# Diretório padrão para monitoramento de XMLs
DIRETORIO_PADRAO=C:\Notas

# Banco de dados
ETL_DATABASE_URL=postgresql://postgres:postgres@localhost:5432/fiscal_datalake

# Comportamento após processamento
DELETAR_APOS_PROCESSAR=true
MOVER_PARA_BACKUP=false
DIRETORIO_BACKUP=C:\SISTEMA\RENSYS\XMLs_Processados

# Validação de duplicatas
VALIDAR_POR_CHAVE=true
VALIDAR_POR_HASH=true

# Processamento
RECURSIVO=true
PROCESSAR_SUBDIRETORIOS=true

# Logs
LOG_NIVEL=INFO
LOG_ARQUIVO=etl_service/logs/etl.log
